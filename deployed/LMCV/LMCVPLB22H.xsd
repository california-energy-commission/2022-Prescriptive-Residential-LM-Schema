<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/LMCVPLB22H" version="2022.0.000" xmlns="http://www.lmonte.com/besm/LMCVPLB22H" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCVPLB22H"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF VERIFICATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="HERS Verified Individual Dwelling Unit Hot Water System Distribution"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderHERS"/>
                     <xsd:element ref="lMCVPLB22H"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF3R"/>
                     <xsd:element name="Footer" type="comp:Footer"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCVPLB22H"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCVPLB22H">
      <xsd:annotation>
         <xsd:documentation>HERS Verified Individual Dwelling Unit Hot Water System Distribution</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Design HERS Verified Dwelling Unit Water Heating Systems Information (other than HPWH)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the LMCC Certificate of Compliance (CC).   This  section is required if the LMCC parent  has any non-HPWH systems, Require one row of data for each non-HPWH system identified on the LMCC</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports features of  water heating system(s) other than HPWH systems that were specified on the registered LMCC compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDesignSystem">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="A01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from LMCC;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A02_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC (see header for rules)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A03_ResidentialWaterHeatingSystemType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC Allowed values are: DHW display term: DHW, HydronicSpaceConditioningDHW display term: Combined Hydronic and Hydronic display term</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:ResidentialWaterHeatingSystemType">
                                             <xsd:enumeration value="DHW"/>
                                             <xsd:enumeration value="Hydronic"/>
                                             <xsd:enumeration value="HydronicSpaceConditioningDHW"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="A04_ResidentialWaterHeaterType" type="hvac:ResidentialWaterHeaterType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water-Heater Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC Allowed values are: BoilerWaterHeater display term: Boiler, IndirectWaterHeating display term:  Indirect, InstantaneousCommercial display term: Commercial Instantaneous, InstantaneousResDutyCommercial display term: Residential-Duty Commercial Instantaneous, InstantaneousConsumer display term: Consumer Instantaneous,   StorageCommercial display term: Commercial Storage, StorageResDutyCommercial display term: Residential-Duty Commercial Storage,   StorageConsumer display term: Consumer Storage</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A05_WaterHeaterTotalSystemCount" type="hvac:WaterHeaterTotalSystemCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of Like (or Identical) Water Heaters in System</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A06_WaterHeaterFuelSource">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fuel Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC Allowed values: Electricity display term Electricity, NaturalGas display term Natural Gas, PropaneGas display term Propane gas, WaterHeatingHP display term Heat Pump</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:WaterHeaterFuelSource">
                                             <xsd:enumeration value="Electricity"/>
                                             <xsd:enumeration value="NaturalGas"/>
                                             <xsd:enumeration value="PropaneGas"/>
                                             <xsd:enumeration value="WaterHeatingHP"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A07_WaterHeaterRatedInputUnits" type="hvac:WaterHeaterRatedInputUnits">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A06_WaterHeaterFuelSource == WaterHeatingHP result = N/A stored in _NotApplicableMessage; Else  reference value from parent CC.  Allowed values: If A06_WaterHeaterFuelSource == NaturalGas Or PropaneGas result = BtuHr Else If A06_WaterHeaterFuelSource == Electricity result = kW</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A06_WaterHeaterFuelSource == WaterHeatingHP result = N/A stored in _NotApplicableMessage; Else  reference value from parent CC.  Allowed values: If A06_WaterHeaterFuelSource == NaturalGas Or PropaneGas result = BtuHr Else If A06_WaterHeaterFuelSource == Electricity result = kW</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="A08_WaterHeaterElectricFiredRatedInput" type="hvac:WaterHeaterElectricFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A06_WaterHeaterFuelSource == WaterHeatingHP result = N/A stored in the NotApplicableMessage;  Else If prescriptive parent CC result = N/A  stored in the NotApplicableMessage; Else If performance parent CC reference value from parent CC.  If A07_WaterHeaterRatedInputUnits == kW store result in WaterHeaterElectricFiredRatedInput Else store result in WaterHeaterGasFiredRatedInput</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A08_WaterHeaterGasFiredRatedInput" type="hvac:WaterHeaterGasFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A06_WaterHeaterFuelSource == WaterHeatingHP result = N/A stored in the NotApplicableMessage;  Else If prescriptive parent CC result = N/A  stored in the NotApplicableMessage; Else If performance parent CC reference value from parent CC.  If A07_WaterHeaterRatedInputUnits == kW store result in WaterHeaterElectricFiredRatedInput Else store result in WaterHeaterGasFiredRatedInput</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="A08_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A06_WaterHeaterFuelSource == WaterHeatingHP result = N/A stored in the NotApplicableMessage;  Else If prescriptive parent CC result = N/A  stored in the NotApplicableMessage; Else If performance parent CC reference value from parent CC.  If A07_WaterHeaterRatedInputUnits == kW store result in WaterHeaterElectricFiredRatedInput Else store result in WaterHeaterGasFiredRatedInput</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="A09_IsDHW_Central" type="comp:IsDHW_Central">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Central DHW System Distribution Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If water heating system in parent CC is Central DHW system result = true display term Yes Else result = false display term NA</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A10_ResidentialDHWDwellingUnitDistributionType" type="hvac:ResidentialDHWDwellingUnitDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit DHW System Distribution Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance parent CC, Then 
If A09_IsDHW_Central == false  allowed values are: StandardDistribution display term: Standard Distribution System; PointOfUse display term: Point Of Use, StandardDistributionCentralParallelPiping display term: Parallel Piping;  RecirculationNonDemand display term:  Recirculation System Non-Demand Control; DemandRecirculationManualControl display term: Demand Recirculation Manual Control;  DemandRecirculationSensorControls display term: Demand Recirculation Sensor Control; StandardDistributionInsulatedPipesHERS display term HERS Verified Pipe Insulation credit, StandardDistributionCentralParallelPipingHERS display term HERS Verified Parallel Piping, DemandRecirculationManualControlHERS display term HERS Verified Demand Recirculation Manual Control, DemandRecirculationSensorControlsHERSHERS display term Verified Demand Recirculation Sensor Controls; 

Else  If A09_IsDHW_Central == true allowed values are: StandardDistribution. Display term: Standard Distribution System, StandardDistributionInsulatedPipesHERS display term HERS Verified Pipe Insulation credit;  
End If

Else If prescriptive parent CC  allowed values are: StandardDistribution display term: Standard Distribution System; DemandRecirculationManualControl display term: Demand Recirculation Manual Control;  DemandRecirculation display term Demand Recirculation;  
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A11_CompactDistributionType" type="hvac:CompactDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compact Distrib.</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC  Allowed values are BasicType display term Basic, Expanded display term Expanded, NotCompact display term None</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="A12_DWHR_Type" type="hvac:DWHR_Type">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Drain Water Heat Recovery</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC  Allowed values are DWHR display term Yes and NoDWHR display term No</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_A2" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Design HERS Verified Dwelling Unit  HPWH System Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the LMCC Certificate of Compliance.This section is required if the LMCC parent  has any HPWH systems, Require one row of data for each HPWH system identified on the LMCC</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A2_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports features of HPWH water heating system(s)  that were specified on the registered LMCC compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableHpwhd">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Hpd01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from LMCC.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd02_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd03_HeatPumpWaterHeaterModel" type="hvac:HeatPumpWaterHeaterModel">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Modeled Equipment Make and Model</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If  prescriptive parent CC result = Tier3NEEA; Else reference value from performance parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd04_WaterHeaterTotalSystemCount" type="hvac:WaterHeaterTotalSystemCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of Like (or Identical) Water Heaters in System</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd05_TankLocation" type="hvac:TankLocation">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd06_ExteriorTankInsulation" type="hvac:ExteriorTankInsulation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Tank Insulation R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from performance parent CC Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd06_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Tank Insulation R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from perofrmance parent CC Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Hpd07_ResidentialDHWDwellingUnitDistributionType" type="hvac:ResidentialDHWDwellingUnitDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit DHW System Distribution Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference parent CC as follows: 
If performance parent CC values are: 
StandardDistribution display term: Standard Distribution System; PointOfUse display term: Point Of Use, StandardDistributionCentralParallelPiping display term: Parallel Piping;  RecirculationNonDemand display term:  Recirculation System Non-Demand Control; DemandRecirculationManualControl display term: Demand Recirculation Manual Control;  DemandRecirculationSensorControls display term: Demand Recirculation Sensor Control; StandardDistributionInsulatedPipesHERS display term HERS Verified Pipe Insulation credit, StandardDistributionCentralParallelPipingHERS display term HERS Verified Parallel Piping, DemandRecirculationManualControlHERS display term HERS Verified Demand Recirculation Manual Control, DemandRecirculationSensorControlsHERS display term HERS Verified Demand Recirculation Sensor Controls; 

Else If prescriptive parent CC values are: StandardDistribution display term: Standard Distribution System; DemandRecirculationManualControl display term: Demand Recirculation Manual Control;   DemandRecirculation display term Demand Recirculation;  
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd08_CompactDistributionType" type="hvac:CompactDistributionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compact Distribution</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC   Allowed values:   
BasicType display term Basic, 
Expanded display term Expanded,
NotCompact display term None</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd09_DWHR_Type" type="hvac:DWHR_Type">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Drain Water Heat Recovery</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from parent CC  Allowed values are DWHR display term Yes and NoDWHR display term None</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd10_HeatPumpWaterHeaterSimulationGroup" type="hvac:HeatPumpWaterHeaterSimulationGroup" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Simulated Equipment Make and Model</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If prescriptive parent CC, this field is not required; Else If performance parent CC reference values from XML   This field IS NOT part of the printed report and is not displayed to the user.  It is used for equivalency lookup</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed HERS Verified Dwelling Unit Water Heating Systems Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If Section_A IsPresent.
Require one row of data for each water heater identified in Section_A TableDesignSystem</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports features of  water heating system(s) other than HPWH systems installed in this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableInstalledSystem">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="B01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Unit Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B02_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A02</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B03_ResidentialWaterHeatingSystemType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A03</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:ResidentialWaterHeatingSystemType">
                                             <xsd:enumeration value="DHW"/>
                                             <xsd:enumeration value="Hydronic"/>
                                             <xsd:enumeration value="HydronicSpaceConditioningDHW"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="B04_ResidentialWaterHeaterType" type="hvac:ResidentialWaterHeaterType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water-Heater Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A04</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B05_WaterHeaterTotalSystemCount" type="hvac:WaterHeaterTotalSystemCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of Like (or Identical) Water Heaters in System</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A05</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B06_WaterHeaterFuelSource">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fuel Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A06</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:WaterHeaterFuelSource">
                                             <xsd:enumeration value="Electricity"/>
                                             <xsd:enumeration value="NaturalGas"/>
                                             <xsd:enumeration value="PropaneGas"/>
                                             <xsd:enumeration value="WaterHeatingHP"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07_WaterHeaterRatedInputUnits" type="hvac:WaterHeaterRatedInputUnits">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from A07</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from A07</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B08_WaterHeaterElectricFiredRatedInput" type="hvac:WaterHeaterElectricFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A08_NotApplicableMeaage != N/A then If B06_WaterHeaterFuelSource == Electricity, user input which must pass following range check: Switch ( B04_ResidentialWaterHeaterType) case  StorageCommercial and case InstantaneousCommercial: result  must be GT 12 kW; case StorageConsumer and case InstantaneousConsumer: result must be LTE 12 kW; case InstantaneousResDutyCommercial: result must be LTE 58.6 kW. Result is stored in WaterHeaterElectricFiredRatedInput;</xsd:documentation>
