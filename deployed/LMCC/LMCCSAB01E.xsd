<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/LMCCSAB01E" version="2022.0.000" xmlns="http://www.lmonte.com/besm/LMCCSAB01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCCSAB01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF COMPLIANCE"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Solar And Battery"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderHERSCC"/>
                     <xsd:element ref="lMCCSAB01E"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF1R" maxOccurs="unbounded"/>
                     <xsd:element name="Footer" type="comp:Footer"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCCSAB01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCCSAB01E">
      <xsd:annotation>
         <xsd:documentation>Solar And Battery</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">GENERAL INFORMATION</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This document is used to demonstrate compliance with prescriptive PV and battery requirements in 
                              <d:t24refnolink/>140.10/
                              <d:t24refnolink/>170.2 for low-rise multifamily and low-rise mixed-use buildings and prescriptive solar thermal requirements in 
                              <d:t24refnolink/>170.2(d)3C for low-rise multifamily and hotel/ motel occupancies.
When PV/battery/solar thermal requirements don't apply or are traded using the performance approach, this document demonstrates compliance with mandatory solar readiness requirements in 
                              <d:t24refnolink/>110.10/ 
                              <d:t24refnolink/>160.8 for newly constructed buildings which are low-rise multifamily or low-rise mixed use. It is also used to demonstrate compliance with solar readiness in 
                              <d:t24refnolink/>160.8 for additions to low-rise multifamily which add more than 2,000 ft<d:sup>2</d:sup> of roof area.  Alterations, or additions of less than 2,000 ft<d:sup>2</d:sup> of roof area, are not required to comply with solar readiness, solar PV and battery requirements and do not need to complete this document.
 </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ClimateZone" type="com:ClimateZone">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from list 1-16</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_ConditionedFloorArea" type="com:ConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>STOP! Roofs designed for vehicle traffic, parking or for heliports are not required to comply with solar ready requirements in 
                                 <d:t24ref h="110.10"/>.  Document this exception by providing the plan sheet showing the roof design in 03a to the right and completing the signature page.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_WholeBuildingOccupancy">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Occupancies</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = MultiFamilyMixedUseLT4 and display term depends on values in A04a_LocalJurisdiction  as follows:
If A04a_LocalJurisdiction == TrinityPublicUtilityDist display term = Multi-family in Trinity Public Utility Dist 
Else If A04a_LocalJurisdiction == CityOfNeedles display term =  Multi-family in City Of Needles 
Else If A04a_LocalJurisdiction == LocalAuthorityNA display term = Multi-family/ Mixed-use
End If</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:WholeBuildingOccupancy">
                           <xsd:enumeration value="MultiFamilyMixedUseLT4"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A04a_LocalJurisdiction" type="comp:LocalJurisdiction">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This field records if the user indicated the project is in an area where CEC approved local building energy standards have been apply which includes 
TrinityPublicUtilityDist, CityOfNeedles and LocalAuthorityNA
 
If result == TrinityPublicUtilityDist   the display term in Trinity Public Utility Dist is appended to the occupancy A04_WholeBuildingOccupancy
Else If result == CityOfNee value indles the display term in City Of Needles is appended to the occupancy  value in A04_WholeBuildingOccupancy
Else If result == LocalAuthorityNA there is no display term appended 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_RoofConstRangeSolar" type="env:RoofConstRangeSolar">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Construction Type</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from list</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_BuildingStoryRange">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:BuildingStoryRange">
                           <xsd:enumeration value="StoriesLTE3"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A05Message" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>STOP!  Alterations or additions that increase roof area by 
                                 <d:lte/> 2,000 ft<d:sup>2</d:sup> are not required to comply with requirements in 
                                 <d:t24refnolink/>110.10/ 
                                 <d:t24refnolink/>170.2(f) and do not need to complete this compliance document.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This message is displayed in User interface  if   A05_RoofConstRangeSolar == AddAltRoofLTE2000ft2</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_RequestedMMB" type="comp:RequestedMMB" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is a flag set by the agent that generates the report XML data to indicate that the user requested a Mandatory Measures Note Block which is placed after the DeclarationBlock in the XML output.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">PROJECT SCOPE</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>The compliance path the project is using to comply per 
                              <d:t24refnolink/>110.10(b)1B/ 
                              <d:t24refnolink/>170.2(f) is indicated below. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BSolarReady" minOccurs="0" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance with Solar Readiness Requirements in 
                           <d:t24refnolink/>110.10(b)1B</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If this subsection is required,  one and only one item can be checked with a result == true and it is the only item that is displayed in the final report.
 
This subsection is required  as follows:
If A04_WholeBuildngOccupancy == MedicalClinicBuilding
BSolarReady is NOT REQUIRED
 
Else If Bpv Is not required and NotPresent
BSolarReady is REQUIRED
Else If Bpv Is required and IsPresent And 
If Bpv01c_RequiredLT1_8kW == true
Or
Bpv01d_NoContiguousAccess == true
Or
Bpv01e_SnowloadException == true
Or
Bpv0g_PerfTradeOffException == true
 BSolarReady is REQUIRED</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="B01a_IsProjectAllocatedSolarZone" type="comp:IsProjectAllocatedSolarZone">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Provide Solar Ready Area no exceptions</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project has allocated a solar zone on the roof plan per requirements in 
                                    <d:t24ref h="110.10(b)"/>, as documented in Table F.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">By default result = false and unchecked.  If user checks it, the result = true,
 
If result == false NOT REPORTED IN PDF
 
Mandatory Measure Triggers:
If result == true
MM110_10b1B_MeasureApplies  = true and is displayed And 
MM110_10b4_MeasureApplies  = true and is displayed And 
DUMM110_10b2_MeasureApplies  = true and is displayed And 
DUMM110_10b3_MeasureApplies  = true and is displayed And 
DUMM110_10b4_MeasureApplies  = true and is displayed And 
DUMM110_10c_MeasureApplies  = true and is displayed And 
DUMM110_10d_MeasureApplies  = true and is displayed And 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="B01d_IsProjectSmartTstatPlusEE_Measure" type="comp:IsProjectSmartTstatPlusEE_Measure">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Exception to Solar Ready Area: Smart Thermostat and Alternative Energy Efficiency Measure</xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">The project is a multifamily occupancy where all thermostats in each dwelling unit comply with 
                                       <d:t24ref h="110.12(a)"/>  AND at least one additional measure listed in Exception 4 to 
                                       <d:t24ref h="110.10(b)1B"/> is installed, as documented in Table I.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">User input
By default result = false and unchecked.  If user checks it, the result = true, 
 
If result == true Section I is triggered 
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="B01d_NotApplicableMessage">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If A04_WholeBuildingOccupancy != MultiFamilyMixedUseLT4  result = BlankGrayedOut stored in NotApplicableMessage</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:element name="B01e_RoofTrafficSRA" type="comp:RoofTrafficSRA">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to Solar Ready Area: Roof is designed for vehicular traffic, parking or for heliport</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Plan sheet showing roof designed for vehicular traffic, parking or heliport</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">By default result = false and unchecked.  If user checks it, the result = true
 
If result == true 
C09_DoesProjectComplySRA == ExceptionalConditionsSRA
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="B01e_BuildingPlanName" type="comp:BuildingPlanName" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">If B01e_RoofTrafficSRA == true this field is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="B01f_RoofTooSmallSRA" type="comp:RoofTooSmallSRA">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to Solar Ready Area: Roof too small</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project is new construction and has a total roof area 
                                    <d:lte/> 533 square feet<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
If A05_RoofConstRangeSolar == NewRoofLTE533ft2
result = true 
Else 
result = false
End If
 
If result == true 
C09_DoesProjectComplySRA == ExceptionalConditionsSRA
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="BEndNote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>1</d:sup>FOOTNOTE: Buildings with roof area 
                                       <d:lte/>533 ft<d:sup>2</d:sup> would have a required solar zone 
                                       <d:lt/> 80 ft<d:sup>2</d:sup> and are therefore exempt per 
                                       <d:t24refnolink/>110.10(b)1. </d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Bpv" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance with Solar Photovoltaic (PV)  Requirements in 
                           <d:t24refnolink/>170.2(f)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  A04_WholeBuildngOccupancy ==  MultiFamilyMixedUseLT4 And A04a_LocalJurisdiction == LocalAuthorityNA And A05_RoofConstRangeSolar == NewRoofLTE533ft2 Or NewRoofGT533ft2
 
If this subsection is required,  one and only one field can be checked with a result == true and it is the only field that is displayed in the final report.   Fields are evaluated in the order they occur in the schema and once a field is evaluated as true and displayed as checked, remaining fields are set to unchecked, result == false and not displayed.</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Bpv01a_SystemIncludesPV" type="comp:SystemIncludesPV">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Provided PV system sized per  
                                    <d:t24refnolink/>170.2 (f)</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project has included an installed PV system per requirements in 
                                    <d:t24refnolink/>170.2(f) as documented in Table J.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: In Section_J
If Pv10_ DesignSize  GTE Pv09_PV_MinSizeSARA
result = true
Else result = false
End If
 
If result == false   
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv01c_RequiredLT1_8kW" type="comp:RequiredLT1_8kW">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: Required PV 
                                    <d:lt/> 1.8kW</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The required PV system size is less than 1.8 kW dc as documented in Table J..</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: In Section_J
If Bpv01a_SystemIncludesPV == true  
result = false
Else (evaluating values from J)
If Pv09_PV_MinSizeSARA LT 1.8
result = true
Else result = false
End If
If result == true 
BSolarReady subsection is triggered
Else If result == false 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv01d_NoContiguousAccess" type="comp:NoContiguousAccess">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: No contiguous Solar Access Roof Area</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The Solar Access Roof Area(s) of the project site contains less than 80 contiguous square feet as documented in Table J.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: In Section_J
If Bpv01a_SystemIncludesPV == true Or  Bpv01c_RequiredLT1_8kW == true
result = false
Else (evaluating values from J)
If Pv05_SolarAccessRoofArea  LT 80
result = true
Else result = false
End If
 
If result == true 
BSolarReady subsection is triggered
Else If result == false 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv01e_SnowloadException" type="comp:SnowloadException">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: Can't meet snow load</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project has a roof design where the enforcement authority has verified it is not possible for the PV system, including panels, modules, components, supports, and attachments to the roof structure, to meet ASCE 7-16 Chapter 7, Snow Loads.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input
If result == true 
BSolarReady subsection is triggered
Else If result == false 
Section_J is triggered And 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv0g_PerfTradeOffException" type="comp:PerfTradeOffException">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">The prescriptive PV/battery requirement has been traded off using the performance compliance approach as documented on the PRF Certificate of Compliance form.</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project is a multi-tenant building in an area where a load serving entity does not provide either a Virtual Net Metering (VNEM) or community solar program.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input
If result == true 
BSolarReady subsection is triggered
Else If result == false And Bpv0e_SnowLoadException And Bpv0f_MultiTenantException == false 
Section_J is triggered  
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Bdhw" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance with Solar Thermal Water Heating Requirements in 
                           <d:t24refnolink/>170.2(d)3C (Multifamiily and hotel/ motel occupancies only)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if user has indicated in UI a gas/propane central water heating system</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Bdhw01a_SystemIncludesSolarDHW" type="comp:SystemIncludesSolarDHW">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">The project includes a hotel/motel or multifamily occupancy with a gas or propane central water-heating system (serves 2+ dwelling units) and includes a permanently installed domestic solar water-heating system to comply with 
                                    <d:t24refnolink/>170.2(d)3C and Reference Residential Appendix RA4, as documented in Table H.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input
If result == true Section_H is required</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">COMPLIANCE RESULTS</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>Results in this table are automatically calculated from data input and calculations in Tables F through I. Note: If any cell on this table says "DOES NOT COMPLY" or "COMPLIES with Exceptional Conditions" refer to Table D. for guidance or see the applicable Table referenced below.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01ToC02ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Allocated Solar Zone</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01ToC02TableRef" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table F)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C01_SolarZoneAreaRequiredMinimum" type="comp:SolarZoneAreaRequiredMinimum">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Required Minimum Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B01a_IsProjectAllocatedSolarZone IsPresent And == true,  
result = F08_SolarZoneAreaRequiredMinimum; 
 
Else result = BlankGrayedOut  stored in NotApplicableMessage and field is grayed out,
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C01_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Required Minimum Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B01a_IsProjectAllocatedSolarZone NotPresent Or B01a_IsProjectAllocatedSolarZone == false  
result =  BlankGrayedOut  stored in NotApplicableMessage and field is grayed out.
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C01AfterSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:lte/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C02_SolarZoneSubAreasTotalSquareFootage" type="comp:SolarZoneSubAreasTotalSquareFootage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Designated Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated  field:  
If B01a_IsProjectAllocatedSolarZone IsPresent And == true And F08_SolarZoneAreaRequiredMinimum GT 0  
result =  FTotals18_SolarZoneSubAreasTotalSquareFootage; 
 
Else result =   BlankGrayedOut  stored in NotApplicableMessage and field is grayed out.
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C02_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Designated Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated  field:  
If B01a_IsProjectAllocatedSolarZone NotPresent Or B01a_IsProjectAllocatedSolarZone == false Or F08_SolarZoneAreaRequiredMinimum == 0  
result =   BlankGrayedOut  stored in NotApplicableMessage and field is grayed out.
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C02a_BuildingPlanName" type="comp:BuildingPlanName">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="AdditionalRequirements">Location within the construction documents showing the location for inverters and metering equipment and a pathway for the routing of conduit/plumbing to the electrical service/ water heating system per 
                              <d:t24ref h="110.10(c)"/>.</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If  B01a_IsProjectAllocatedSolarZone IsPresent And == true, 
result =  F20_BuildingPlanName; 
Else 
result =  NotApplicable display term Not Applicable stored in NotApplicableMessage and field is grayed out.
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C02a_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If B01a_IsProjectAllocatedSolarZone NotPresent Or B01a_IsProjectAllocatedSolarZone == false
result =  NotApplicable display term Not Applicable stored in NotApplicableMessage and field is grayed out.
End If</xsd:documentation>
                           <xsd:appinfo source="displayterm">
                              <dtyp:displayterm value="NotApplicable">Not Applicable</dtyp:displayterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C02AfterSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">OR</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C03ToC04ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Installed PV System</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C03To04TableRef" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Tables G or J)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C03_PV_SystemTotalRatedPowerDC_Minimum" type="hvac:PV_SystemTotalRatedPowerDC_Minimum">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Required Minimum DC Power Rating (Watts)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Section_J IsPresent
result = Pv09_PV_MinSizeSARA x 1000
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C03_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Required Minimum DC Power Rating (Watts)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Section_J NotPresent
result =  BlankGrayedOut  stored in NotApplicableMessage and field is grayed out.
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C03AfterSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:lte/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C04_PV_SystemTotalRatedPowerDC" type="hvac:PV_SystemTotalRatedPowerDC">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Designed DC Power Rating (Watts)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Section_J IsPresent
result = Pv10_PV_DesignSize x 1000
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C04_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Designed DC Power Rating (Watts)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If  Section_J NotPresent
result =  BlankGrayedOut  stored in NotApplicableMessage and field is grayed out. 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C04AfterSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">OR</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C05ToC06ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Installed SWH System</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C05To06TableRef" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table H)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C05_SolarWaterHeatingSavingsFractionRequired" type="comp:SolarWaterHeatingSavingsFractionRequired">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Required Minimum Solar Savings Fraction</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If  Section_H IsPresent
If H00c_IsDWHR_Installed == false 
If A02_ClimateZone InRange (1,9) 
result = 0.2; 
Else If A02_ClimateZone InRange (10,16) 
result = 0.35;  
End If
 
Else If  H00c_IsDWHR_Installed == true 
If A02_ClimateZone InRange (1,9) 
result = 0.15; 
Else If A02_ClimateZone InRange (10,16) 
result = 0.30
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C05_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Required Minimum Solar Savings Fraction</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If  Section_H NotPresent
result =  BlankGrayedOut  stored in NotApplicableMessage and field is grayed out; 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C05AfterSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:lte/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C06_SolarWaterHeatingSavingsFraction" type="comp:SolarWaterHeatingSavingsFraction">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Designed/Rated Solar Savings Fraction</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If  Section_H IsPresent 
result = H03_SolarSavingsFractionOG100
End if</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C06_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Designed/Rated Solar Savings Fraction</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If  Section_H NotPresent
result =  BlankGrayedOut  stored in NotApplicableMessage and field is grayed out; 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C06AfterSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">OR</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C07ToC08ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Smart Tstat and Alternative EE Measure</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C07To08TableRef" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table I)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C07_IsOCST_RequirementSatisfied" type="comp:IsOCST_RequirementSatisfied">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">JA5 Compliant Thermostat Specified?</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Section_I IsPresent
If I01a_IsOCST_RequirementSatisfied == true and I01b_BuildingPlanName NotNull, 
result = true, display term Yes, 
Else result = false display term No
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C07_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">JA5 Compliant Thermostat Specified?</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If Section_I NotPresent
result = BlankGrayedOut  stored in NotApplicableMessage and field is grayed out; 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C08_SolarReadyAlternativeEfficiencyMeasure" type="comp:SolarReadyAlternativeEfficiencyMeasure" maxOccurs="unbounded">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Alternative Energy Efficiency Measure</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Section_I IsPresent 
If I02a_SolarReadyAlternativeEfficiencyMeasure And I02b_SolarReadyAlternativeEfficiencyMeasure And I02c_SolarReadyAlternativeEfficiencyMeasure And I02d_SolarReadyAlternativeEfficiencyMeasure And I02e_SolarReadyAlternativeEfficiencyMeasure == NoEfficiencyMeasure 
result = NoEfficiencyMeasure with no display term leaving the field blank
 
Else If I02f_BuildingPlanName NotNull 
If I02a_SolarReadyAlternativeEfficiencyMeasure == EnergyStarAppliances 
result +=  EnergyStarAppliances 
End If
If I02b_SolarReadyAlternativeEfficiencyMeasure == HomeAutomationDemandResponseCapable  
result +=  HomeAutomationDemandResponseCapable 
End If
If I02c_SolarReadyAlternativeEfficiencyMeasure == AlternativePlumbingForGrayWaterIrrigation  
result +=   AlternativePlumbingForGrayWaterIrrigation
End If
If  I02d_SolarReadyAlternativeEfficiencyMeasure == RainwaterCatchmentSystem  
result +=  RainwaterCatchmentSystem 
End If
If  I02e_SolarReadyAlternativeEfficiencyMeasure == ChargingSpaceForEV  
result +=  ChargingSpaceForEV  
End If
End If</xsd:documentation>
                           <xsd:appinfo source="displayterm">
                              <dtyp:displayterm value="EnergyStarAppliances">Energy Star Appliances and EC Motor</dtyp:displayterm>
                              <dtyp:displayterm value="HomeAutomationDemandResponseCapable">Home Automation System</dtyp:displayterm>
                              <dtyp:displayterm value="AlternativePlumbingForGrayWaterIrrigation">Greywater Irrigation System</dtyp:displayterm>
                              <dtyp:displayterm value="RainwaterCatchmentSystem">Rainwater Catchment System</dtyp:displayterm>
                              <dtyp:displayterm value="ChargingSpaceForEV">Part 11: Electric Vehicle Charging</dtyp:displayterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C08_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Alternative Energy Efficiency Measure</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If Section_I NotPresent
result = BlankGrayedOut  stored in NotApplicableMessage and field is grayed out; 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C09_DoesProjectComplySRA" type="comp:DoesProjectComplySRA">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Compliance Results</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field:  
If C01_SolarZoneAreaRequiredMinimum IsPresent then  
If  C02_SolarZoneSubAreasTotalSquareFootage GTE C01_SolarZoneAreaRequiredMinimum And all values in F19_DoesSolarZoneSubAreaQualify = true, 
result = CompliantSRA display term COMPLIES; 
 
Else If C01_SolarZoneAreaRequiredMinimum == 0 And C02_NotApplicableMessage == NotApplicable  
result = CompliantSRA display term COMPLIES; 
Else result = NotCompliantSRA display term DOES NOT COMPLY;  
End If
Else If C01_NotApplicableMessage IsPresent
Continue evaluation
 
If C03_PV_SystemTotalRatedPowerDC_Minimum IsPresent then 
If C04_PV_SystemTotalRatedPowerDC  GTE C03_PV_SystemTotalRatedPowerDC_Minimum 
result = CompliantSRA display term COMPLIES; 
Else result = NotCompliantSRA display term DOES NOT COMPLY;   
End if
Else If C03_NotApplicableMessage IsPresent
Continue evaluation
 
If C05_SolarWaterHeatingSavingsFractionRequired IsPresent then 
If C06_SolarWaterHeatingSavingsFraction GTE C05_SolarWaterHeatingSavingsFractionRequired 
result = CompliantSRA display term COMPLIES; 
Else result = NotCompliantSRA display term DOES NOT COMPLY;   
End If
Else If C05_NotApplicableMessage IsPresent
Continue evaluation
 
If C07_IsOCST_RequirementSatisfied IsPresent then 
If C07_IsOCST_RequirementSatisfied == true And C08_SolarReadyAlternativeEfficiencyMeasure NotNull And C08_SolarReadyAlternativeEfficiencyMeasure != NoEfficiencyMeasure,  then 
result = CompliantSRA display term COMPLIES; 
Else result = NotCompliantSRA display term DOES NOT COMPLY
End If
Else If C07_NotApplicableMessage IsPresent
Continue evaluation
 
If B01e_RoofTrafficSRA == true Or B01f_RoofTooSmallSRA == true Or B01g_StoriesTooManySRA == true
result = ExceptionalConditionsSRA display term COMPLIES with Exceptional Conditions
Else result = NotCompliantSRA display term DOES NOT COMPLY;
End If
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C09a_SolarZoneConnectionsDocCompliance" type="comp:SolarZoneConnectionsDocCompliance">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If  B01a_IsProjectAllocatedSolarZone == false then 
result = NotApplicable stored in C09a_NotApplicableMessage;  
 
Else If C02a_BuildingPlanName NotNull, then 
result = Complies display term COMPLIES; 
Else result = DoesNotComply display term DOES NOT COMPLY
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C09a_NotApplicableMessage" type="comp:NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">If  B01a_IsProjectAllocatedSolarZone == false then 
result = NotApplicable display term Not Applicable 
End If</xsd:documentation>
                           <xsd:appinfo source="displayterm">
                              <dtyp:displayterm value="NotApplicable">Not Applicable</dtyp:displayterm>
                           </xsd:appinfo>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C09b_DoesProjectComplyBattery">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Battery storage system design meets the minimum requirements in Joint Appendix JA12 and the minimum energy (kWh)/  power (kW) capacity per Table J.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field: 
result = NotApplicableBat display term Not Applicable</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:DoesProjectComplyBattery">
                           <xsd:enumeration value="NotApplicableBat"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">EXCEPTIONAL CONDITIONS</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table is auto-filled with uneditable comments because of selections made or data entered in tables throughout the form.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableAllExceptionalConditions" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_SectionExceptionalConditionNotes" type="comp:SectionExceptionalConditionNotes">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"/>
                                          <xsd:documentation source="CalculationsAndRules">This combines all the strings stored in in ExceptionalConditionNote for sections that have exceptional conditions</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_AllSectionsErrorMessages" type="comp:AllSectionsErrorMessages" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"/>
                                          <xsd:documentation source="CalculationsAndRules">This combines all the strings stored in in SectionErrorMessage  for sections that have errors</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="D03_ExceptionalConditionMessageCCSABE" type="comp:ExceptionalConditionMessageCCSABE" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">ADDITIONAL REMARKS</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the permit applicant to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table is includes remarks made by the permit applicant to the Authority Having Jurisdiction.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">ALLOCATED SOLAR ZONE</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Section F is required if B01a_IsProjectAllocatedSolarZone == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table is completed if the project is designating a solar zone to comply with 
                              <d:t24ref h="110.10(b)1B"/>. New construction consider the total roof area; Additions consider newly added roof area.  This table demonstrates that the project has designated the minimum area required for the Allocated Solar Zone, and also that the requirements for Solar Zone Subareas have been met.  Each subarea must be shown on a roof plan or documented in construction documents.  The solar zones must also comply with fire code requirements, including, but not limited to, setback and pathway requirements.  Requirements for interconnection pathways must also be included in construction documents, and the location is specified in this table. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F01To08ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Required Minimum Solar Zone</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F01_SolarZoneAreaCalculationMethod" type="comp:SolarZoneAreaCalculationMethod">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Minimum Solar Zone Area Calculation Method</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: PotentialSolarZoneMethod display term Potential Solar Zone, RoofNewAddedAreaMethod display term Total New of Added Roof area</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02_RoofAreaNewOrAdded" type="comp:RoofAreaNewOrAdded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total New or Added Roof Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F03_SkylightAreaRoofNewOrAdded" type="comp:SkylightAreaRoofNewOrAdded">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Total New or Added Roof Area Covered with Skylights (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required if F01_SolarZoneAreaCalculationMethodNR == RoofNewAddedAreaMethod; Else NotApplicable stored in F03_NotApplicableMessage and field is grayed out.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F03_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Total New or Added Roof Area Covered with Skylights (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required if F01_SolarZoneAreaCalculationMethodNR == RoofNewAddedAreaMethod; Else NotApplicable stored in F03_NotApplicableMessage and field is grayed out.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F04_SolarZoneAreaNetRoofAreaMethod" type="comp:SolarZoneAreaNetRoofAreaMethod">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Minimum Solar Zone Based on Total or Added Roof Area (0.15 x (Roof-Skylt)) (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: If F01_SolarZoneAreaCalculationMethodNR == RoofNewAddedAreaMethod  result = PRODUCT (0.15, (F02_RoofAreaNewOrAdded -  F03_SkylightAreaRoofNewOrAdded));  Else result = NotApplicable stored in F04_NotApplicableMessage and field is grayed out;</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F04_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Minimum Solar Zone Based on Total or Added Roof Area (0.15 x (Roof-Skylt)) (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: If F01_SolarZoneAreaCalculationMethodNR == RoofNewAddedAreaMethod  result = PRODUCT (0.15, (F02_RoofAreaNewOrAdded -  F03_SkylightAreaRoofNewOrAdded));  Else result = NotApplicable stored in F04_NotApplicableMessage and field is grayed out;</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F05_SolarAccessMethodDescription" type="comp:SolarAccessMethodDescription">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Method/ Tools Used to Determine Annual Solar Access for Potential Zones<d:sup>1</d:sup>
                           </xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod; Else result =  BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F05_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Method/ Tools Used to Determine Annual Solar Access for Potential Zones<d:sup>1</d:sup>
                           </xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod; Else result =  BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="F06aToF06cColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Potential Solar Zone Areas: Roof areas with 
                           <d:gte/> 70% Solar Access</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F06a_RoofAreaLowSlope70PercentSolarAccess" type="comp:RoofAreaLowSlope70PercentSolarAccess">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Low-Sloped Area (
                              <d:lte/> 2:12 pitch) (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod; Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F06a_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Low-Sloped Area (
                              <d:lte/> 2:12 pitch) (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod; Else result =BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F06b_RoofAreaSteepSlope70PercentSolarAccess" type="comp:RoofAreaSteepSlope70PercentSolarAccess">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Steep-Sloped Area (
                              <d:gt/> 2:12 pitch) Oriented 90
                              <d:deg/> - 300 
                              <d:deg/> (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod; Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F06b_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Steep-Sloped Area (
                              <d:gt/> 2:12 pitch) Oriented 90
                              <d:deg/> - 300 
                              <d:deg/> (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input required If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod; Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F06c_SolarZoneAreaPotential" type="comp:SolarZoneAreaPotential">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Total Potential Solar Zone Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod  result = SUM (F06a_RoofAreaLowSlope70PercentSolarAccess, F06b_RoofAreaSteepSlope70PercentSolarAccess); Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F06c_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Total Potential Solar Zone Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod  result = SUM (F06a_RoofAreaLowSlope70PercentSolarAccess, F06b_RoofAreaSteepSlope70PercentSolarAccess); Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F07_SolarZoneAreaSolarAccessMethod" type="comp:SolarZoneAreaSolarAccessMethod">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Minimum Solar Zone Based on Potential Zone (0.5 x (Total Potential Zone)) (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod result = PRODUCT (0.5, F06c_SolarZoneAreaPotential); Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F07_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Minimum Solar Zone Based on Potential Zone (0.5 x (Total Potential Zone)) (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: If F01_SolarZoneAreaCalculationMethodNR == PotentialSolarZoneMethod result = PRODUCT (0.5, F06c_SolarZoneAreaPotential); Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="F08_SolarZoneAreaRequiredMinimum" type="comp:SolarZoneAreaRequiredMinimum">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Required Minimum Solar Zone Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value:  If F01_SolarZoneAreaCalculationMethodNR == RoofNewAddedAreaMethod result = F04_SolarZoneAreaNetRoofAreaMethod Else result = F07_ SolarZoneAreaSolarAccessMethod</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F09To19ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b> Designated Solar Zone Subareas </d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ChoiceSubAreas">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:choice>
                           <xsd:element name="F09to19NA_NotApplicableMessage">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">If F08_SolarZoneAreaRequiredMinimum == 0 result = BlankGrayedOut for this subsection.  No data is required for F09 - F19 and the fields are blank and grayed out</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:NotApplicableMessage">
                                    <xsd:enumeration value="BlankGrayedOut"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="SubAreas">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">If F08_SolarZoneAreaRequiredMinimum GT 0 data on solar Zone Subareas is required.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="TableSubArea">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"/>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="F09_SolarZoneSubAreaID" type="comp:SolarZoneSubAreaID">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Subarea Name or Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F10_BuildingPlanName" type="comp:BuildingPlanName">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Building Plan Reference</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F11_RoofSlope">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Roof or Overhang Slope (Low 
                                                               <d:lte/> 2:12 pitch) (Steep 
                                                               <d:gt/> 2:12 pitch)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list: LowSlope  
 SteepSlope</xsd:documentation>
                                                            <xsd:appinfo source="displayterm">
                                                               <dtyp:displayterm value="LowSlope">Low-sloped</dtyp:displayterm>
                                                               <dtyp:displayterm value="SteepSlope">Steep-sloped</dtyp:displayterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="env:RoofSlope">
                                                               <xsd:enumeration value="LowSlope"/>
                                                               <xsd:enumeration value="SteepSlope"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="F12_IsSolarZoneSubAreaSteepSlopeAzimuthCompliant" type="comp:IsSolarZoneSubAreaSteepSlopeAzimuthCompliant">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Is Steep-Sloped Roof or Overhang between 90 and 300 degrees?</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If F11_RoofSlope != SteepSlope result = NotApplicable stored in F12_NotApplicableMessage; Else User input.   If user selects Yes, result = true, else If user selects No result = false
 
Mandatory Measure Triggers:
If result == true
MM110_10b2_MeasureApplies  = true and is displayed
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="F12_NotApplicableMessage">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Is Steep-Sloped Roof or Overhang between 90 and 300 degrees?</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If F11_RoofSlope != SteepSlope result = NotApplicable stored in F12_NotApplicableMessage; Else User input.   If user selects Yes, result = true, else If user selects No result = false</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="F13_IsSolarZoneSubAreaT24Part9Compliant" type="comp:IsSolarZoneSubAreaT24Part9Compliant">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Subarea Complies with Title 24, Part 9</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input.  If user selects Yes, result = true, else If user selects No result = false</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F14_IsSolarZoneSubAreaObstructionsFree" type="comp:IsSolarZoneSubAreaObstructionsFree">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Solar Zone Subarea Free of Obstructions per 
                                                               <d:t24ref h="110.10(b)3A"/>
                                                            </xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input.  If user selects Yes, result = true, else If user selects No result = false
 
Mandatory Measure Triggers:
If result == true
MM110_10b3_MeasureApplies  = true and is displayed
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F15_IsSolarZoneSubAreaDistantFromObstructions" type="comp:IsSolarZoneSubAreaDistantFromObstructions">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Subarea is Required Distance from Potential Obstructions per 
                                                               <d:t24ref h="110.10(b)3B"/>
                                                            </xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input.  If user selects Yes, result = true, else If user selects No result = false
 
Mandatory Measure Triggers:
If result == true
MM110_10b3_MeasureApplies  = true and is displayed
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F16_IsSolarZoneSubAreaSmallDimension5ftPlus" type="comp:IsSolarZoneSubAreaSmallDimension5ftPlus">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Is the Smallest Dimension 5 feet or greater?</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input.  If user selects Yes, result = true, else If user selects No result = false</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F17_SolarZoneSubAreaSizeRequiredMinimum" type="comp:SolarZoneSubAreaSizeRequiredMinimum">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Min. Area Required per Subarea (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value: If F02_RoofAreaNewOrAdded LTE 10,000 ft2 result = 80 ft2; Else result = 160 ft2</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F18_SolarZoneSubAreaSquareFootage" type="comp:SolarZoneSubAreaSquareFootage">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Designated Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="F19_DoesSolarZoneSubAreaQualify" type="comp:DoesSolarZoneSubAreaQualify">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Subarea Complies?</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value: If F12_IsSolarZoneSubAreaSteepSlopeAzimuthCompliant == true Or F12_NotApplicableMessage == NotApplicable, And If F13_IsSolarZoneSubAreaT24Part9Compliant ==true And F14_ IsSolarZoneSubAreaObstructionsFree
 ==true And F15_ IsSolarZoneSubAreaDistantFromObstructions==true And F16_ IsSolarZoneSubAreaSmallDimension5ftPlus == true And F18_SolarZoneSubAreaSquareFootage GTE F17_SolarZoneSubAreaSizeRequiredMinimum Then result = true display term COMPLIES Else result = false display term  DOES NOT COMPLY and D03_ExceptionalConditionMessageCCSABE += SubAreaComplianceInF</xsd:documentation>
                                                            <xsd:appinfo source="displayterm">
                                                               <dtyp:displayterm value="true">COMPLIES</dtyp:displayterm>
                                                               <dtyp:displayterm value="false">DOES NOT COMPLY</dtyp:displayterm>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="FTotals18_SolarZoneSubAreasTotalSquareFootage" type="comp:SolarZoneSubAreasTotalSquareFootage" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">
                                             <d:b>Total Designated Solar Zone Area (ft<d:sup>2</d:sup>):</d:b>
                                          </xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If F08_SolarZoneAreaRequiredMinimum GT 0 result = SUM All (F18_SolarZoneSubAreaSquareFootage   where F19_DoesSolarZoneSubAreaQualify == true ) Else no data required and in display field is grayed out</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:choice>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="F20_BuildingPlanName" type="comp:BuildingPlanName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Interconnection Pathways</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Location in construction documents showing the location for inverters and metering equipment and a pathway for the routing of conduit/ plumbing to the electrical service/ water heating system per 
                           <d:t24ref h="110.10(c)"/>.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input
 
Mandatory Measure Triggers:
If result == true
MM110_10c_MeasureApplies  = true and is displayed And
MM110_10d_MeasureApplies  = true and is displayed And 
 MM110_10e_MeasureApplies  = true and is displayed And 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements"><d:sup>1</d:sup>FOOTNOTE:  This field is used to document how the percentage of annual solar access was determined per 
                           <d:t24ref h="110.10(b)1B"/>.  Solar access is the ratio of solar insolation including shade to the solar insolation without shade.  Shading from obstructions located on the roof or any other part of the building shall not be included in the determination of annual solar access.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">PERMANENTLY INSTALLED SOLAR PV FOR SOLAR READY EXCEPTION</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Section G is not required for the LMCC, so no data is collected and only the section title and Begin Note are displayed</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This section does not apply to low-rise multifamily projects.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">PERMANENTLY INSTALLED SOLAR HOT WATER SYSTEMS</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Section H is required If Bdhw01a_SystemIncludesSolarDHW == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table is completed if the project is installing a solar water heating (SWH) system to comply with 
                              <d:t24refnolink/>110.10(b)1B Exception 2 or for multifamily/ hotel/motel occupancies to demonstrate compliance with 
                              <d:t24refnolink/>170.2(d)3C. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H00a" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">OG-100 Certified Collectors</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The SWH system is built-up and specifies collectors certified by the Solar Rating &amp;amp; Certification Company (SRCC) or the International Association of Plumbing &amp;amp; Mechanical Officials (IAPMO) as OG-100</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H00c_IsDWHR_Installed" type="comp:IsDWHR_Installed">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Drain Water Heat Recovery</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">A drain water heat recovery system that is field verified by a HERS Rater as specified in the Reference Appendix RA3.6.9 is being used to lower the required Solar Savings Fraction per 
                           <d:t24refnolink/>170.2(d)3Cii.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H01_SolarWaterHeatingCertificationAuthority" type="comp:SolarWaterHeatingCertificationAuthority">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">SRCC# or IAPMO File #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from list: IAPMOfile#, 
 SRCC#</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="H02_SolarWaterSystemSRCC_CertificationNumber" type="hvac:SolarWaterSystemSRCC_CertificationNumber">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Certification #</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input; If H01_SolarWaterHeatingCertificationAuthority ==SRCC# user input is assigned to H02_SolarWaterSystemSRCC_CertificationNumber</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="H02_SolarWaterSystemIAPMO_FileNumber" type="hvac:SolarWaterSystemIAPMO_FileNumber">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Certification #</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">User input; If H01_SolarWaterHeatingCertificationAuthority ==IAPMOfile# user input is assigned to H02_SolarWaterSystemIAPMO_FileNumber</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="H03_SolarSavingsFractionOG100" type="comp:SolarSavingsFractionOG100">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Designed Solar Savings Fraction for OG-100</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H04_IsT24SWHCalculatorOutputAttached" type="comp:IsT24SWHCalculatorOutputAttached">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Output from CEC's T24 SWH Calculator Attached<d:sup>1</d:sup>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input  If user selects Yes result = true
If result == false
D03_ExceptionalConditionMessageCCSABE += CalculatorOutputAttachedForH</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H06_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This field is displayed as  Pass checkbox and a Fail checkbox in printed form that is given to the Field Inspector.  It is currently not part of the stored XML data</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>1</d:sup>FOOTNOTES: Built-up SWH systems using OG-100 collectors must calculate the Designed Solar Savings Fraction using the SWH module in the CEC's CBECC software.  The calculation output must be included within the permit application. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">SMART THERMOSTATS AND ALTERNATIVE EFFICIENCY MEASURE FOR SOLAR READY EXCEPTION</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Section I is required if B01d_IsProjectSmartTstatPlusEE_Measure == true</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Only 1 I02 element should be != NoEfficiencyMeasure</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="I_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>  This table indicates thermostats compliant with 
                              <d:t24refnolink/>110.12(a) have been specified and which additional energy efficiency measure has been included in the design for multifamily occupancies complying with Exception 4 to 
                              <d:t24refnolink/>110.10(b)1B .</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Smart Thermostats</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01a_IsOCST_RequirementSatisfied" type="comp:IsOCST_RequirementSatisfied">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The contract documents clearly specify that all thermostats in each dwelling unit comply with 
                           <d:t24refnolink/>110.12(a) prior to granting of an occupancy permit by the enforcing agency.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01b_BuildingPlanName" type="comp:BuildingPlanName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Location in contract documents:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I02ColumnHeading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Alternative Efficiency Measure</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I02ColumnSubheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">In addition, my project includes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I02a_SolarReadyAlternativeEfficiencyMeasure">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Energy Star Appliances and EC Motor</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The contract documents include an ENERGY STAR dishwasher AND EITHER an ENERGY STAR refrigerator OR a whole house fan driven by an electronically commutated motor</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If unchecked result = NoEfficiencyMeasure, Else result =  EnergyStarAppliances</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="EnergyStarAppliances">Energy Star Appliances and EC Motor</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:SolarReadyAlternativeEfficiencyMeasure">
                           <xsd:enumeration value="EnergyStarAppliances"/>
                           <xsd:enumeration value="NoEfficiencyMeasure"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="I02b_SolarReadyAlternativeEfficiencyMeasure">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Home Automation System</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The contract documents include a home automation system that is capable of, at a minimum, controlling the appliances and lighting of the dwelling and responding to demand response signals.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If unchecked result = NoEfficiencyMeasure, Else result =  HomeAutomationDemandResponseCapable</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="HomeAutomationDemandResponseCapable">Home Automation System</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:SolarReadyAlternativeEfficiencyMeasure">
                           <xsd:enumeration value="HomeAutomationDemandResponseCapable"/>
                           <xsd:enumeration value="NoEfficiencyMeasure"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="I02c_SolarReadyAlternativeEfficiencyMeasure">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Greywater Irrigation System</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The contract documents include alternative plumbing piping to permit the discharge from the clothes washer and all showers and bathtubs to be used for an irrigation system in compliance with the California Plumbing Code and any applicable local ordinances.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If unchecked result = NoEfficiencyMeasure, Else result =  AlternativePlumbingForGrayWaterIrrigation</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="AlternativePlumbingForGrayWaterIrrigation">Greywater Irrigation System</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:SolarReadyAlternativeEfficiencyMeasure">
                           <xsd:enumeration value="AlternativePlumbingForGrayWaterIrrigation"/>
                           <xsd:enumeration value="NoEfficiencyMeasure"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="I02d_SolarReadyAlternativeEfficiencyMeasure">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Rainwater Catchment System</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The contract documents include a rainwater catchment system designed to comply with the California Plumbing Code and any applicable local ordinances, and that uses rainwater flowing from at least 65% of the available roof area.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If unchecked result = NoEfficiencyMeasure, Else result =  RainwaterCatchmentSystem</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="RainwaterCatchmentSystem">Rainwater Catchment System</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:SolarReadyAlternativeEfficiencyMeasure">
                           <xsd:enumeration value="NoEfficiencyMeasure"/>
                           <xsd:enumeration value="RainwaterCatchmentSystem"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="I02e_SolarReadyAlternativeEfficiencyMeasure">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Part 11: Electric Vehicle Charging</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The contract documents demonstrate compliance with Title 24, Part 11 
                           <d:t24refnolink/>A4.106.8.2 requirements for electric vehicle charging spaces.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If unchecked result = NoEfficiencyMeasure, Else result =  ChargingSpaceForEV</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="ChargingSpaceForEV">Part 11: Electric Vehicle Charging</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:SolarReadyAlternativeEfficiencyMeasure">
                           <xsd:enumeration value="ChargingSpaceForEV"/>
                           <xsd:enumeration value="NoEfficiencyMeasure"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="I02f_BuildingPlanName" type="comp:BuildingPlanName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Location in contract documents:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">PHOTOVOLTAIC (PV) SYSTEMS</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if Bpv01e_SnowLoadException == false And Bpv0g_PerfTradeOffException == false And A04a_LocalJurisdiction == LocalAuthorityNA</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Pv_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>  This table documents compliance with prescriptive photovoltaic system requirements in 
                              <d:t24refnolink/>170.2(f). Unless the project meets one of the listed exceptions, or trades-off PV in an energy model using performance path, 
                              <d:t24refnolink/>170.2(g and h) requires installed photovoltaic systems for newly constructed buildings.  The installed PV systems must meet the minimum requirements in Joint Appendix 11.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="PvSubtitle" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Photovoltaic (PV) Systems</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv01_RoofArea" type="env:RoofArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Area of New Roof<d:sup>1</d:sup> (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input area</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv02_RoofArea70PercentSolarAccess" type="comp:RoofArea70PercentSolarAccess">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Roof Area 
                           <d:lt/> 70% Solar Access<d:sup>2</d:sup> (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input area</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv03_BuildingPlanName" type="comp:BuildingPlanName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Plansheet or Document showing Solar Access Calculations</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv04_RoofAreaOccupied" type="comp:RoofAreaOccupied">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Occupied Roof Area<d:sup>3</d:sup> (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv05_SolarAccessRoofArea" type="comp:SolarAccessRoofArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Solar Access Roof Area (SARA) (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field:
result = Pv01_RoofArea - Pv02_RoofArea70PercentSolarAccess - Pv04_RoofAreaOccupied</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv06_PV_MaxSizeSARA" type="comp:PV_MaxSizeSARA">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Max Size of PV system that will fit on SARA (kWdc)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv07_DwellingUnitsCount" type="com:DwellingUnitsCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"># of Dwelling Units</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User inpt</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv08_BatteryCapacity" type="hvac:BatteryCapacity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Usable Capacity of Battery Storage<d:sup>4</d:sup>  (kWh)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input battery capacity (kWh) which could be 0</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv09_PV_MinSizeSARA" type="comp:PV_MinSizeSARA">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Min Size of PV System Required (kWdc)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field:
result = Smallest (Pv06_PV_MaxSizeSARA,, Equation result )
If result == Equation result And Pv08_BatteryCapacity GTE 7.5
D03_ExceptionalConditionMessageCCSABE += BatteryCapacityReducesPV
 
 
Equation result = ((A03_ConditionedFloorArea x FactorA) / 1000 + (Pv07_DwellingUnitsCount  x FactorB))
 
FactorA  and FactorB assigned as follows:
 
Switch (A02_ClimateZone)
case 1:
FactorA = 0.793
FactorB = 1.27
case 2:
FactorA = 0.621
FactorB = 1.22
case 3: 
FactorA = 0.628
FactorB = 1.12
case 4:
FactorA = 0.586
FactorB = 1.21
case 5:
FactorA = 0.585
FactorB = 1.06
case 6:
FactorA = 0.594
FactorB = 1.23
case 7:
FactorA = 0.572
FactorB = 1.15
case 8:
FactorA = 0.586
FactorB = 1.37
case 9:
FactorA = 0.613
FactorB = 1.36
case 10: 
FactorA = 0.627
FactorB = 1.41
case 11:
FactorA = 0.836
FactorB = 1.44
case 12:
FactorA = 0.613
FactorB = 1.40
case 13:
FactorA = 0.894
FactorB = 1.51
case 14:
FactorA = 0.741
FactorB = 1.26
case 15
FactorA = 1.56
FactorB = 1.47
case 16: 
FactorA = 0.59
FactorB = 1.22 
End Switch
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pv10_PV_DesignSize" type="comp:PV_DesignSize">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Size of PV system in Design (kWdc)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="PvEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>1</d:sup>FOOTNOTES:  Includes the area of the building's roof space capable of structurally supporting a PV system and the area of all roof space on covered parking areas, carports, and all other newly constructed structures on the site that are compatible with supporting a PV system per Title 24, Part 2 Section 1511.2.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="PvEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>2</d:sup>Solar access must be determined using CEC approved solar access calculation tools found at https://www.energy.ca.gov/programs-and-topics/programs/building-energy-efficiency-standards/solar-assessment-tools.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="PvEndNote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>3</d:sup>As specified by CBC Section 503.1.4.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="PvEndNote4" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>4</d:sup>PV system sizes determined using Equation 170.2-C may be reduced by 25% if installed in conjunction with a battery storage system.  The battery storage system shall meet the qualification requirements specified in Joint Appendix JA12 and have a minimum usable capacity of 7.5 kWh</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Lmci" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">DECLARATION OF REQUIRED CERTIFICATES OF INSTALLATION</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Values in this section are affected by data in other sections of the document which may change the result to true and select Yes radio button.  
 
If value is set to true by the software and the user changes this value from true to false, an explanation is required in Section E Additional Remarks And 
D03_ExceptionalConditionMessageCCSABE += UserChangedSelectionInCI</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Lmci_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>Selections have been made based on information provided in this document. If any selections have been changed by the permit applicant,  an explanation should be included Table E. Additional Remarks and ExceptionalConditionMessageCCSABE += UserChangedSelectionInCI.  

These documents must be provided to the building inspector during construction and can be found online  </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Lmci01_RequireLMCISab01E" type="comp:RequireLMCISab01E">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">LMCI-SAB-01-E - Must be submitted for all buildings that must comply with solar readiness or PV/Battery requirements.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">result = true display term Yes and can not be changed by the user</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Lmci01a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Lmci02a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Lmca" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">DECLARATION OF REQUIRED CERTIFICATES OF ACCEPTANCE</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Lmca_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>There are no Certificates of Acceptance applicable to solar readiness or PV/battery requirements.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="lMCCSAB01E" type="LMCCSAB01E">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the LMCCSAB01E compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
