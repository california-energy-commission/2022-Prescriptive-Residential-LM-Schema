                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-PRC-02-F Kitchen Exhaust</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-MCH parent
result = Q02_RequireLMCAPrc02F 
If result == true 
static text and related SystemFieldVerified  or SystemsToFieldVerify are displayed
Else If result == false
Nothing is displayed for this NRCA 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G02_SystemFieldVerified" type="comp:SystemFieldVerified"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Q02_RequireNRCAPrc02F == false; this field is not required and nothing is displayed
Else If Q02_RequireNRCAPrc02F == true
result = Q02_SystemFieldVerified and it is displayed
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
