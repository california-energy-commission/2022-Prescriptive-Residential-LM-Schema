<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/LMCIPRC01E"
   xmlns="http://www.lmonte.com/besm/LMCIPRC01E"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2022.0.000">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCIPRC01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF INSTALLATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Process Systems"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderHERS" />
                     <xsd:element ref="lMCIPRC01E" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF2R_H" maxOccurs="unbounded"/>
                     <xsd:element name="Footer" type="comp:Footer" />
                     <xsd:element name="ComplianceID" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220901"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCIPRC01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCIPRC01E">
      <xsd:annotation>
         <xsd:documentation>Process Systems</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]A. GENERAL INFORMATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This  Certificate of Installation is used to document the installation of mechanical features, materials, components, and manufactured devices required to demonstrate compliance with Title 24, Part 6 per [d:t24refnolink/]110-103(a)3 for nonresidential, hotel/motel and high-rise residential occupancies.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If NRCC-MCH parent, 
result = A01_ProjectCity
Else If  NRCC-PRF parent, 
result = Info01_ProjectCity
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="NRCCPRC01E"  usage="copy">A01_ProjectCity</dtyp:srcSchema>
                           <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">Info04_ProjectCity</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ProjectZipcode" type="comp:ProjectZipcode" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip Code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If NRCC-PRF parent
result = Info02_ProjectZipcode
Else If NRCC-MCH parent
User input
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">Info06_ProjectZipcode</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_PermitSetDate" type="comp:PermitSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_AuthorityHavingJurisdiction" type="comp:AuthorityHavingJurisdiction" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Authority Having Jurisdiction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_PermitNumber" type="comp:PermitNumber" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Permit #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AsBuiltSetDate" type="comp:AsBuiltSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_PermitSetName" type="comp:PermitSetName"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_AsBuiltSetName" type="comp:AsBuiltSetName"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]B. INSTALLER SCOPE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates construction systems and materials documented on this Certificate of Installation.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Electrical</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01a_ProjectHasElevatorLighting" type="comp:ProjectHasElevatorLighting" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Elevator Lighting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">HVAC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02c_ProjectHasEnclosedParking" type="comp:ProjectHasEnclosedParking" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Parking Garage Exhaust</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02d_ProjectHasCommercialKitchen" type="comp:ProjectHasCommercialKitchen" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Commercial Kitchen Ventilation</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02e_ProjectHasElevatorVentilation" type="comp:ProjectHasElevatorVentilation" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Elevator Ventilation Controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B06_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Specialty</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input
If checked result = true
Else result = false
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B06a_ProjectHasKitchenHood" type="comp:ProjectHasKitchenHood" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Commercial Kitchen Hood</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input
If checked result = true
Else result = false
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B06e_ProjectHasPoolSpa" type="comp:ProjectHasPoolSpa" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pool / Spa</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input
If checked result = true
Else result = false
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C. COMPLIANCE RESULTS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates whether the as-built conditions documented in this form are equal or better than what was documented on the permitted Certificate of Compliance.  If the installation is not equal or better, Section 10-103(a)2B requires the Certificate of Compliance form to be revised accordingly to demonstrate compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ComplianceResultCI" type="comp:ComplianceResultCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If in any required tables or subsections in Section F  ComplianceResultNRCI == TableIncomplete
result = FormNotComplete
Else If in any required tables or subsections in Section F ComplianceResultNRCI == MayNotComply
result = MayNotComply
Else If All required tables or subsections in Section F  have  ComplianceResultNRCI ==  ExactMatch
result = ExactMatch
Else If any required tables or subsections in Section F have ComplianceResultNRCI ==  EqualBetter
result = EqualBetter
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01Message_ComplianceResultMessageCI" type="comp:ComplianceResultMessageCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If C01_ComplianceResultNRCI == ExactMatch Or EqualBetter
result = ExactEqualBetterText
Else If C01_ComplianceResultNRCI == MayNotComply
result =  MayNotComplyText
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]D. EXCEPTIONAL CONDITIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is auto-filled with uneditable comments because of field conditions noted by the installer that may impact requirements documented on the Certificate of Compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageCIPRCE" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document
If ExceptionalConditionMessageNRCIPRCE == ""
result = NoExceptionNote
End If</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:ExceptionalConditionMessageCIPRCE">
                           <xsd:enumeration value="ElevLtiNotInScope"/>
                           <xsd:enumeration value="HoodsNotInScope"/>
                           <xsd:enumeration value="NoExceptionNote"/>
                           <xsd:enumeration value="ParkNotInScope"/>
                           <xsd:enumeration value="UserChangedCASelection"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]E. INSTALLER NOTES[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the permit applicant to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes remarks made by the installer to the Authority Having Jurisdiction.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F. INSTALLATION DETAILS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]The following tables indicate performance requirements as documented on the permitted Certificate of Compliance (NRCC) for all systems and components included in Table B. Installer Scope.  Also included are the as-built conditions documented by the installer/documentation author. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelTopCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Per NRCC</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the top cell for all tables/subsections in Section F except the Ducts subsection and the Pipes subsection</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelBottomCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">As-built Conditions</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the bottom cell for all tables/subsections in Section F except the Ducts subsection and the Pipes subsection</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Park" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Enclosed Parking Garage Exhaust[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if B02c_ProjectHasEnclosedParking == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TablePark" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Enclosed Parking Garage Exhaust [/d:b]</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Park01cc_FanName" type="hvac:FanName" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Fan Name</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If NRCC-PRC parent
If H10_FanName IsPresent
result = H10_FanName
End If
End If</xsd:documentation>
                                                      <xsd:appinfo source="CIsrc">
                                                         <dtyp:srcSchema form="NRCCPRC01E"  usage="copy">H10_FanName</dtyp:srcSchema>
                                                         <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">ProcGarage01_ExhaustFanName</dtyp:srcSchema>
                                                         <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">ProcGarage01_ExhaustFanName</dtyp:srcSchema>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Park01cc_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Fan Name</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If ParkCtrl01cc_VehiclesGT20PctNonGas == true  Or ParkCtrl02cc_NewExhaustLT10000cfm == true 
result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Park01_InMyScope" type="comp:InMyScope" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Is this in your scope?</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from true value display term Yes and false value display term No, by others
 If result == false Park02cc-Park03cc  and Park02-Park03 As-built fields are blank and grayed out
And D01_ExceptionalConditionMessageNRCIPRCE += ParkNotInScope  
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="ParkScopeChoice" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="ParkScopeNA_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Park01_InMyScope == false
result = BlankGrayedOut for Park02cc-Park03cc  and Park02-Park03 As-built fields are blank and grayed out 
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="ParkInScope" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Park01_InstalledSameAsCC" type="comp:InstalledSameAsCC"  minOccurs="0">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                                     <xsd:documentation source="AdditionalRequirements">[d:i]This table includes all newly installed mechanical ventilation systems which serve parking garages and have a design exhaust rate greater than or equal to 10,000 cfm.  The inputs of Table H are used to demonstrate compliance with the requirements of [d:t24refnolink/]120.6(c) [/d:i]</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as NRCC value  Or false if installed is different than NRCC value</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Park02cc_ParkingGarageArea" type="com:ParkingGarageArea" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Parking Garage Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If NRCC-PRC parent
If H11_ParkingGarageArea IsPresent
result = H11_ParkingGarageArea
End If
End If</xsd:documentation>
                                                                        <xsd:appinfo source="CIsrc">
                                                                           <dtyp:srcSchema form="NRCCPRC01E"  usage="copy">H11_ParkingGarageArea</dtyp:srcSchema>
                                                                        </xsd:appinfo>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Park02cc_NotApplicableMessage" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Parking Garage Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If ParkCtrl01cc_VehiclesGT20PctNonGas == true  Or ParkCtrl02cc_NewExhaustLT10000cfm == true 
result = BlankGrayedOut</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Park02_ParkingGarageArea" type="com:ParkingGarageArea" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Parking Garage Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If ParkCtrl01_VehiclesGT20PctNonGas == false And ParkCtrl02_NewExhaustLT10000cfm == false
If Park02cc_ParkingGarageArea IsPresent
If Park01_InstalledSameAsCC == true
result = Park02cc_ParkingGarageArea
Else If Park01_InstalledSameAsCC == false
User input
End If
Else
User input
End If
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Park02_NotApplicableMessage" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Parking Garage Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If ParkCtrl01_VehiclesGT20PctNonGas == true  Or ParkCtrl02_NewExhaustLT10000cfm == true 
result = BlankGrayedOut</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Park03cc_FanFlowCapacity" type="hvac:FanFlowCapacity" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ventilation Fan Rate (CFM)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If NRCC-PRC parent
If H12_FanFlowCapacity IsPresent
result = H12_FanFlowCapacity
End If
End If</xsd:documentation>
