                                    <xsd:element name="F11_ThermostatType" type="hvac:ThermostatType"  fixed="SetbackThermostat">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Required Thermostat Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Result = SetbackThermostat</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
