         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCCPLB01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCCPLB01E">
      <xsd:annotation>
         <xsd:documentation>Domestic Water Heating System</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">GENERAL INFORMATION</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This document is used to demonstrate compliance with requirements in [d:t24refnolink/]110.1, [d:t24refnolink/]110.3, [d:t24refnolink/]120.3, and [d:t24refnolink/]140.5, and with requirements in [d:t24refnolink/]141.0 for additions and alterations, for domestic water heating systems serving nonresidential occupancies within low-rise mixed use buildings  using the prescriptive path. For low-rise residential and hotel/motel occupancies compliance is demonstrated with requirements in [d:t24refnolink/]110.1, [d:t24refnolink/]110.3, [d:t24refnolink/]160.4 and [d:t24refnolink/]170.2(d), and with requirements [d:t24refnolink/]180.1 for additions and [d:t24refnolink/]180.2 for alterations.[/d:i]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This note should be placed after the header and before section A</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
