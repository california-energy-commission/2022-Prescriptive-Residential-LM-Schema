   </xsd:element>
   <xsd:complexType name="LMCCPRC01E">
      <xsd:annotation>
         <xsd:documentation>Process Systems</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">GENERAL INFORMATION</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This form is used to document any process systems that are within the scope of the permit application and are demonstrating compliance with mandatory requirements in [d:t24refnolink/]120.6/ [d:t24refnolink/]160.7  or prescriptive requirements in [d:t24refnolink/]140.9. This compliance document is used for newly constructed, addition and alteration projects.[/d:i]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This note should be placed after the header and before section A</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ClimateZone" type="com:ClimateZone" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input
 
Mandatory Measure Triggers:
If result == 2 Or 4 Or 6 Or 7 Or 8 Or 9 Or 10 Or 11 Or 13 Or 15 And 
If Fcon01Adiabatic _CondenserTypeApplies == false 
MM120_6a4F_MeasureApplies = true and is displayed 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_WholeBuildingOccupancy"  maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Occupancy Types Within Project:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects as many types as apply and they are stored in this element</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:WholeBuildingOccupancy">
                           <xsd:enumeration value="AllOthersBuildings"/>
                           <xsd:enumeration value="AuditoriumBuilding"/>
                           <xsd:enumeration value="CommercialIndustrialBuilding"/>
                           <xsd:enumeration value="ConventionCenterBuilding"/>
                           <xsd:enumeration value="DataCenterBuilding"/>
                           <xsd:enumeration value="FinancialInstitutionBuilding"/>
                           <xsd:enumeration value="GroceryStoreBuilding"/>
                           <xsd:enumeration value="GymnasiumBuilding"/>
                           <xsd:enumeration value="HospitalBuilding"/>
                           <xsd:enumeration value="HotelMotel"/>
                           <xsd:enumeration value="LibraryBuilding"/>
                           <xsd:enumeration value="LowRiseResidential"/>
                           <xsd:enumeration value="MedicalClinicBuilding"/>
                           <xsd:enumeration value="OfficeBuilding"/>
                           <xsd:enumeration value="ParkingGarageBuilding"/>
                           <xsd:enumeration value="ReligiousFacilityBuilding"/>
                           <xsd:enumeration value="RelocatablePublicSchoolBuilding"/>
                           <xsd:enumeration value="RestaurantBuilding"/>
                           <xsd:enumeration value="RetailBuilding"/>
                           <xsd:enumeration value="SchoolBuilding"/>
                           <xsd:enumeration value="SupportArea"/>
                           <xsd:enumeration value="TheaterBuilding"/>
                           <xsd:enumeration value="UnleasedTenantSpace"/>
                           <xsd:enumeration value="Warehouse"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A04_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Conditioned Floor Area</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_TotalUnconditionedFloorArea" type="com:TotalUnconditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Unconditioned Floor Area</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AboveGradeStoryCount" type="env:AboveGradeStoryCount" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"># of Stories (Habitable  Above Grade)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_RequestedMMB" type="comp:RequestedMMB"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is a flag set by the agent that generates the report XML data to indicate that the user requested a Mandatory Measures Note Block which is placed after the DeclarationBlock in the XML output.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A10_IsProjectAllElectric" type="comp:IsProjectAllElectric" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A11_GasType" type="com:GasType" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Values are NaturalGas, Propane and None</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="AEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Alert</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b] [d:i]Alert!  Healthcare Facilities do not have to meet the elevator, commercial kitchen, or lab exhaust requirements under Title 24, Part 6 and therefore are not documented on the CC-PRC-E.  The corresponding tables (K, N, O) say "This section does not apply" when healthcare facility has been chosen as an occupancy within Table A.  Systems serving these spaces shall meet the requirements of the Appliance Efficiency Regulations for walk-in coolers or freezers contained in the Appliance Efficiency Regulations (California Code of Regulations, Title 20, Sections 1601 through 1608).  [/d:i][/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This alert is displayed If A03_WholeBuildingOccupancy == HospitalBuilding</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
