   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCCPRC01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF COMPLIANCE"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Process Systems"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderHERSCC" />
                     <xsd:element ref="lMCCPRC01E" />
                     <xsd:element name="Section_MandatoryMeasureBlock" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation source="FieldText">[d:b][d:i]Covered Process Mandatory Measures:[/d:i][/d:b]</xsd:documentation>
									<xsd:documentation source="CalculationsAndRules">If LMCC-PRC-E is included in scope And 
A07_RequestedMMB == true
this Mandatory Measures Noteblock is required 
 
Else it is not required and nothing is captured or displayed
End If</xsd:documentation>
									<xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="MM_BeforeNote1" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="FieldText">[d:b][d:i]2022 Nonresidential Energy Standards Compliance (Title 24, Part 6, Ch. 1)[/d:i][/d:b]</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM_BeginNote1" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="FieldText">[d:i]The following mandatory measures have been identified as applicable to the project and must be met in order to demonstrate compliance with Title 24, Part 6[/d:i]</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="FieldText">[d:b][d:t24refnolink/]120.6(c) ENCLOSED PARKING GARAGES [/d:b]</xsd:documentation>
												<xsd:documentation source="AdditionalRequirements">MECHANICAL VENTILATION SYSTEMS FOR ENCLOSED PARKING GARAGES WHERE THE TOTAL DESIGN EXHAUST RATE FOR THE GARAGE IS [d:gte/] 10,000 CFM SHALL:</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If MM120_6c1_MeasureApplies Or MM120_6c2_MeasureAppliesOr MM120_6c3_MeasureApplies Or MM120_6c4_MeasureApplies Or MM120_6c5_MeasureApplies  Or MM120_6c6_MeasureApplies Or MM120_6c7_MeasureApplies  == true
 
this static text is displayed in the report once as a heading  for these measures
Else
this static text is not displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c1_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">1. AUTOMATICALLY DETECT CONTAMINANT LEVELS AND STAGE FANS OR MODULATE FAN AIRFLOW RATES TO 50% OR LESS OF DESIGN CAPACITY</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If H03_FanControlContaminantLevels == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c2_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">2. HAVE CONTROLS/ DEVICES THAT WILL RESULT IN FAN MOTOR DEMAND OF NO MORE THAN 30% OF DESIGN WATTAGE AT 50% DESIGN AIRFLOW.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If H04_FanControlMotorDemand  == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c3_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">3. MONITOR CO WITH AT LEAST ONE SENSOR PER 5,000 FT2 LOCATED IN THE HIGHEST EXPECTED CONCENTRATION LOCATIONS WITH AT LEAST TWO SENSORS PER PROXIMITY ZONE.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If H05_MonitoringCO  == true And H06_SensorLocationCO  == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c4_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">4. MAINTAIN CO CONCENTRATION AT 25 PPM OR LESS AT ALL TIMES.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If H07_SensorSetpointCO  == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c5_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">5. MAINTAIN VENTILATION RATE AT AT LEAST 0.15 CFM/FT2 WHEN THE GARAGE IS SCHEDULED TO BE OCCUPIED.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If H09_GarageTotalVentRate  == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c6_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">6. MAINTAIN NEGATIVE OR NEUTRAL PRESSURE RELATIVE TO OTHER OCCUPIABLE SPACES WHEN THE GARAGE IS SCHEDULED TO B OCCUPIED.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If H08_GarageNegativePressurization  == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c7_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">7. CO SENSORS SHALL BE: [d:l t="*"][d:list]CERTIFIED BY THE MANUFACTURER TO BE ACCURATE WITHIN PLUS OR MINUS 5%  [/d:list][d:list]DRIFT NO MORE THAN 5% PER YEAR AND REQUIRE CALIBRATION NO MORE THAN ONCE PER YEAR, AND [/d:list][d:list]BE FACTORY CALIBRATED  [/d:list][d:list]MONITORED BY A CONTROL SYSTEM WITH LOGIC THAT AUTOMATICALLY CHECKS FOR SENSOR FAILURE. UPON DETECTION OF A FAILURE, SYSTEM SHALL RESET TO DESIGN VENTILATION RATES AND TRANSMIT AN ALARM TO THE FACILITY OPERATORS[/d:list][/d:l]</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If H05_MonitoringCO  == true And H06_SensorLocationCO  == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6c8_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText">[d:b][d:t24refnolink/]120.6(c)8 PARKING GARAGE VENTILATION SYSTEM ACCEPTANCE [/d:b]</xsd:documentation>
												<xsd:documentation source="AdditionalRequirements">BEFORE AN OCCUPANCY PERMIT IS GRANTED FOR A PARKING GARAGE SYSTEM A CERTIFICATE OF ACCEPTANCE SHALL BE SUBMITTED TO THE ENFORCEMENT AGENCY THAT CERTIFIES THAT THE EQUIPMENT AND SYSTEMS MEET THE ACCEPTANCE REQUIREMENTS SPECIFIED IN NONRESIDENTIAL REFERENCE APPENDICES NA7.12.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If B01_ProjectHasEnclosedParking  == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6cExc1_ExceptionApplies" type="comp:ExceptionApplies" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
												<xsd:documentation source="AdditionalRequirements">[d:b]EXCEPTION 1[/d:b] TO [d:t24refnolink/]120.6(c) [d:b]APPLIES[/d:b]:

GARAGES (OR PORTION OF GARAGE) WHERE [d:gt/] 20% OF THE STORED VEHICLES HAVE NON-GASOLINE COMBUSTION ENGINES</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If MM120_6c1_MeasureApplies Or MM120_6c2_MeasureAppliesOr MM120_6c3_MeasureApplies Or MM120_6c4_MeasureApplies Or MM120_6c5_MeasureApplies  Or MM120_6c6_MeasureApplies Or MM120_6c7_MeasureApplies  == true Or MM120_6c8_MeasureApplies  == true
 
user input
Else
result = false
End If
 
If result == true  FieldText and AdditionalRequirements static text is displayed in the report
Else If result == false nothing is displayed in the report
End if</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6cExc2_ExceptionApplies" type="comp:ExceptionApplies" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
												<xsd:documentation source="AdditionalRequirements">[d:b]EXCEPTION 2[/d:b] TO [d:t24refnolink/]120.6(c) [d:b]APPLIES[/d:b]:

ADDITIONS AND ALTERATIONS TO EXISTING GARAGES WHERE [d:lt/] 10,000 CFM OF NEW EXHAUST CAPACITY IS ADDED.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If MM120_6c1_MeasureApplies Or MM120_6c2_MeasureAppliesOr MM120_6c3_MeasureApplies Or MM120_6c4_MeasureApplies Or MM120_6c5_MeasureApplies  Or MM120_6c6_MeasureApplies Or MM120_6c7_MeasureApplies  == true Or MM120_6c8_MeasureApplies  == true
 
user input
Else
result = false
End If
 
If result == true  FieldText and AdditionalRequirements static text is displayed in the report
Else If result == false nothing is displayed in the report
End if</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6f_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText">[d:b][d:t24refnolink/]120.6(f) MANDATORY REQUIREMENTS FOR ELEVATORS[/d:b]</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If MM120_6f1_MeasureApplies Or MM120_6f2_MeasureApplies Or MM120_6f3to5_MeasureApplies == true
 
this static text is displayed in the report once as a heading  for these measures
Else
this static text is not displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6f1_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">1. THE LIGHT POWER DENSITY FOR THE LUMINAIRES INSIDE THE ELEVATOR CAB SHALL BE NO GREATER THAN 0.6 WATTS PER SQUARE FOOT.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If Section K is required because B01_ProjectHasElevatorControls == true. 
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6f1Exc1_ExceptionApplies" type="comp:ExceptionApplies" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
												<xsd:documentation source="AdditionalRequirements">[d:b]EXCEPTION 1[/d:b] TO [d:t24refnolink/]120.6(f)1 [d:b]APPLIES[/d:b]:

INTERIOR SIGNAL LIGHTING AND INTERIOR DISPLAY LIGHTING ARE NOT INCLUDED IN THE CALCULATION OF LIGHTING POWER DENSITY.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If MM120_6f1_MeasureApplies == true 
user input
Else
result = false
End If
 
If result == true  FieldText and AdditionalRequirements static text is displayed in the report
Else If result == false nothing is displayed in the report
End if</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6f2_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">2. ELEVATOR CAB VENTILATION FANS FOR CABS WITHOUT SPACE CONDITIONING SHALL NOT EXCEED 0.33 WATTS PER CFM AS MEASURED AT MAXIMUM SPEED.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If K10_IsSpaceConditioned   == false 
 
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6f3to5_MeasureApplies" type="comp:MeasureApplies">
											<xsd:annotation>
												<xsd:documentation source="FieldText"/>
												<xsd:documentation source="AdditionalRequirements">3. WHEN STOPPED AND UNOCCUPIED WITH DOORS CLOSED FOR OVER 15 MINUTES, THE CAB INTERIOR LIGHTING AND VENTILATION FANS SHALL BE SWITCHED OFF.  [d:line1/]4. LIGHTING AND VENTILATION SHALL REMAIN OPERATIONAL IN THE EVENT THAT THE ELEVATOR CABIN GETS STUCK WHEN PASSENGERS ARE IN THE CABIN.  [d:line1/]5. BEFORE AN OCCUPANCY PERMIT IS GRANTED FOR ELEVATORS, A CERTIFICATE OF ACCEPTANCE SHALL BE SUBMITTED TO THE ENFORCEMENT AGENCY THAT CERTIFIES THAT THE EQUIPMENT AND SYSTEMS MEET THE ACCEPTANCE REQUIREMENTS SPECIFIED IN NONRESIDENTIAL REFERENCE APPENDICES NA7.14.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If Section K is required because B01_ProjectHasElevatorControls == true
result = true and FieldText and AdditionalRequirements static text is 
displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MM120_6fExc_ExceptionApplies" type="comp:ExceptionApplies" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
												<xsd:documentation source="AdditionalRequirements">[d:b]EXCEPTION[/d:b] TO [d:t24refnolink/]120.6(f) [d:b]APPLIES[/d:b]: 

ELEVATORS IN HEALTHCARE FACILITIES.</xsd:documentation>
												<xsd:documentation source="CalculationsAndRules">If MM120_6f_MeasureApplies == true Or MM120_6f1_MeasureApplies Or MM120_6f2_MeasureApplies Or MM120_6f3to5_MeasureApplies == true
user input
Else
result = false
End If
 
If result == true  FieldText and AdditionalRequirements static text is displayed in the report
Else If result == false nothing is displayed in the report
End if</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF1R" maxOccurs="unbounded" />
                     <xsd:element name="Footer" type="comp:Footer" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
